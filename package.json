{
  "name": "reverse-with-nextjs-material",
  "version": "4.0.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "latest",
    "@material-ui/icons": "^4.9.1",
    "clsx": "latest",
    "dotenv": "latest",
    "next": "latest",
    "node-mocks-http": "^1.10.1",
    "pg": "^8.6.0",
    "prop-types": "latest",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "swr": "^0.5.6"
  },
  "scripts": {
    "pg-start": "pg_ctl -D /usr/local/var/postgres -l /usr/local/var/postgres/server.log start",
    "pg-stop": "pg_ctl -D /usr/local/var/postgres stop -s -m fast",
    "pg-createdb": "createdb reverse",
    "pg-data": "psql -d sample -f pg-data.sql",
    "test": "jest",
    "dev": "next",
    "build": "next build && ./pg-db.sh reverse && psql -d reverse -f pg-data.sql",
    "start": "next start"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "should": "^13.2.3",
    "jest": "^26.6.3"
  }
